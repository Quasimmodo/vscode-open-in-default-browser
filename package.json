{
  "name": "open-html-in-browser",
  "displayName": "Open HTML In Default Browser",
  "description": "Open HTML In Default Browser",
  "version": "2.0.0-alpha",
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./dist/test/runTest.js"
  },
  "publisher": "peakchen90",
  "engines": {
    "vscode": "^1.38.0"
  },
  "icon": "public/icon.jpg",
  "activationEvents": [
    "onCommand:peakchen90.openInBrowser"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "peakchen90.openInBrowser",
        "title": "%extension.openInBrowser.title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == html",
          "command": "peakchen90.openInBrowser",
          "group": "1_modification"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceLangId == html",
          "command": "peakchen90.openInBrowser",
          "group": "1_modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "peakchen90.openInBrowser",
        "key": "ctrl+1",
        "mac": "cmd+1",
        "when": "editorTextFocus"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peakchen90/vscode-open-in-default-browser.git"
  },
  "bugs": {
    "url": "https://github.com/peakchen90/vscode-open-in-default-browser/issues"
  },
  "keywords": [
    "open",
    "html",
    "default",
    "browser",
    "chrome"
  ],
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.18",
    "@types/vscode": "^1.38.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "typescript": "^3.6.3",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "getmac": "^1.4.6",
    "open": "^6.4.0"
  }
}
